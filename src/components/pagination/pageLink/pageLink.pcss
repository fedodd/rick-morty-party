.pageLink {
  padding: 2px 5px 1px 5px;
  margin: 0 5px;
  color: var(--color-main);
  border: 1px solid var(--color-main);

  &.is__active {
    color: black;
    border-color: black;
  }

  &:hover {
    cursor: pointer;
    color: var(--color-dark);
    border-color: var(--color-dark);
  }
}
